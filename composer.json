{
  "name": "burdadirect/symfony-bundle-twig-bootstrap",
  "description": "HBM Twig Bootstrap Bundle - Bootstrap helper for twig.",
  "authors" : [{
    "name" : "Christian Puchinger",
    "email" : "christian.puchinger@burda.com"
  }],
  "license": "MIT",
  "type": "symfony-bundle",
  "autoload": {
    "psr-4": {
      "HBM\\TwigBootstrapBundle\\": "src/"
    }
  },
  "repositories" : [
    {
      "type": "vcs",
      "url": "git@github.com:burdadirect/symfony-bundle-twig-attributes.git"
    },
    {
      "type": "vcs",
      "url": "git@github.com:burdadirect/codestyle.git"
    }
  ],
  "require": {
    "php": "^8.2.0",
    "burdadirect/symfony-bundle-twig-attributes": "dev-main",
    "symfony/config": "^6.3",
    "symfony/dependency-injection": "^6.3",
    "symfony/twig-bundle": "^6.3"
  },
  "require-dev": {
    "burdadirect/codestyle": "dev-main",
    "roave/security-advisories": "dev-latest"
  },
  "config": {
    "allow-plugins": {
      "phpstan/extension-installer": true
    }
  }
}
